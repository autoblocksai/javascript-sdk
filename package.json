{
  "name": "@autoblocks/client",
  "version": "0.0.0-automated",
  "description": "JavaScript client for Autoblocks",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./langchain": {
      "require": "./dist/langchain/index.js",
      "import": "./dist/langchain/index.mjs"
    }
  },
  "typesVersions": {
    "*": {
      "langchain": [
        "./dist/langchain/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .ts --max-warnings 0 .",
    "test": "TZ=UTC jest",
    "test:e2e": "tsx e2e/test-api-client/index.ts",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/autoblocksai/javascript-sdk",
  "bugs": "https://github.com/autoblocksai/javascript-sdk/issues",
  "author": "Autoblocks",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "langchain": "^0.0.165",
    "lint-staged": "^13.2.3",
    "openai": "^4.11.1",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "zod": "^3.21.4"
  }
}
