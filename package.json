{
  "name": "@autoblocks/client",
  "version": "0.0.0-automated",
  "description": "JavaScript client for Autoblocks",
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "build": "npm run clean && npm run build:main && npm run build:module",
    "lint": "eslint --ext .ts --max-warnings 0 .",
    "test": "jest",
    "prepare": "husky install"
  },
  "homepage": "https://github.com/autoblocksai/javascript-sdk",
  "bugs": "https://github.com/autoblocksai/javascript-sdk/issues",
  "author": "Autoblocks",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0"
  }
}
